cc_library_static {
    name: "libLLVM10Support",
    vendor_available: true,
    defaults: ["llvm10-defaults-no-generated-headers"],
    target: {
        // llvm10-defaults turns off host builds by default, turn it back on for
        // modules that are used to build llvm10-tblgen
        host: {
            enabled: true,
        },
        windows: {
            enabled: true,
        },
    },
    srcs: [
        "*.c",
        "*.cpp",
    ],
}
